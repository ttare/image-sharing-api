{
  "name": "image-sharing-api",
  "description": "Starter project",
  "scripts": {
    "compile": "babel server/ -d server.lib/ --presets es2015",
    "start": "npm run watch",
    "db:migrate": "sequelize db:migrate --migrations-path server.lib/migrations",
    "build": "babel config server -s -D -d dist --presets es2015,stage-0",
    "test": "eslint server",
    "watch": "nodemon -w server --exec \"npm run compile && node server.lib\""
  },
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "debug": "^2.6.0",
    "express": "^4.13.3",
    "express-session": "^1.15.0",
    "express-validation": "^1.0.2",
    "express-validator": "^5.2.0",
    "express-winston": "^2.1.3",
    "joi": "^9.0.4",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.7.0",
    "multer": "^1.3.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.0",
    "sequelize-cli": "^2.5.1",
    "sqlite3": "^3.1.8",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "l": "^0.6.0",
    "nodemon": "^1.18.3"
  }
}
